<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>GivePenny</Authors>
    <Company>GivePenny</Company>
    <Product>Pat</Product>
    <Description>Monitoring extension for Pat to report metrics to Application Insights.</Description>
    <Copyright>GivePenny</Copyright>
    <PackageLicenseUrl>https://github.com/GivePenny/Pat.Subscriber.Telemetry.ApplicationInsights/License.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/GivePenny/Pat.Subscriber.Telemetry.ApplicationInsights</PackageProjectUrl>
    <RepositoryUrl>https://github.com/GivePenny/Pat.Subscriber.Telemetry.ApplicationInsights</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Pat</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="5.0.0" />
    <PackageReference Include="Pat.Subscriber.NetCoreDependencyResolution" Version="4.0.43" />
    <PackageReference Include="Microsoft.ApplicationInsights" Version="2.17.0" />
  </ItemGroup>

</Project>
